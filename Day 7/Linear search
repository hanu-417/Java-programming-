class LinearSearch {
    public static void main(String[] args) {
        int[] a = {5, 8, 2, 9, 1};
        int key = 9;

        boolean found = false;
        for (int i : a) {
            if (i == key) {
                found = true;
                break;
            }
        }

        if (found)
            System.out.println("Element Found");
        else
            System.out.println("Not Found");
    }
}
